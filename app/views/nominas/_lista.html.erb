<% if nomina.present? %>
  
  <% nomina.each do |n| %>

    <tr>

      <td class="text-min"><%= n.ano_periodo_pago %></td>
      <td class="text-min"><%= obtener_mes(n.mes_periodo_pago) %></td>
      <td class="text-min"><%= n.codigo_trabajador %></td>
      <td class="text-min"><%= n.nombre_trabajador %></td>
      <td class="text-min"><%= n.nombre_objeto_gasto %></td>
      <td class="text-min"><%= n.estado %></td>
      <td class="text-min" style="text-align:right;"><%= number_to_currency(n.asignacion, :unit => '', :precision => 0) %></td>
      <td style="width:20px;"><%= link_to_nominas_detalles(n) %></td>
       
    </tr>
      
  <% end %>

