<h2>
	<span itemprop="name">Matriculaciones Educación Media</span>
  <meta itemprop="description" content="
    Se refiere a la cantidad de matriculados del 1.º al 3er. curso en las diferentes modalidades de educación media con sus correspondientes códigos identificatorios."/>
  <meta itemprop="url" content="http://datos.mec.gov.py/data/matriculaciones_educacion_media">
  <meta itemprop="temporal" style="display: none;" content="2012-01-01/2012-12-31"/>
  <span itemprop="catalog" itemscope itemtype="https://schema.org/DataCatalog" style="display: none;">
    <meta itemprop="name" content="Datos Abiertos del Ministerio de Educación y Cultura">
    <meta itemprop="url" content="http://datos.mec.gov.py/">
    <meta itemprop="description" content="Datos Abiertos del Ministerio de Educación y Cultura">
  </span>
  <span itemprop="provider" itemscope itemtype="https://schema.org/Organization" style="display: none;">
    <meta itemprop="name" content="Ministerio de Educación y Cultura">
    <meta itemprop="url" content="http://www.mec.gov.py/">
    <meta itemprop="email" content="datosabiertos@mec.gov.py">
  </span>
  <span itemprop="creator" itemscope itemtype="https://schema.org/Person" style="display: none;">
    <meta itemprop="name" content="Juan Barrios">
    <meta itemprop="email" content="datosabiertos@mec.gov.py">
  </span>
  <meta itemprop="license" style="display: none;" content="https://creativecommons.org/licenses/by/4.0/legalcode"/>
  <meta itemprop="keywords" style="display: none;" content="educación,matriculaciones,educación media"/>
  <meta itemprop="version" style="display: none;" content="1.0"/>
  
  <span style="float:right;">
    <%= link_to "Ver diccionario", mec_custom_url(def_matriculaciones_educacion_media_url), :title => "Ir al diccionario", :style => "font-size:12px;" %>
    <%= link_to image_tag("def_icon.png", :style => "width:24px;" ), mec_custom_url(def_matriculaciones_educacion_media_url), :title => "Ir al diccionario" %>
    <%= link_to "Inicio", mec_custom_url(index_url), :title => "Ir al inicio", :style => "font-size:12px;" %>
    <%= link_to image_tag("home.png", :style => "width:24px;"), mec_custom_url(index_url), :title => "Ir al inicio" %>
  </span>

</h2>

<div class="table-outter">
  <table id="matriculaciones-educacion-media-table" class="table table-striped table-bordered table-hover" style="margin-bottom: 0px; width: 2200px; max-width: none">

    <tr>

      <%= form_tag mec_custom_url(data_matriculaciones_educacion_media_lista_url(nil,nil)), :remote => true, :id => "form-buscar-matriculaciones-educacion-media" do  %>

        <th style="width:6%" class="text-min celda-derecha">
          <%= label_tag "Periodo" %><br />
          <%= select :form_buscar_matriculaciones_educacion_media, :anio, [2012], {:class => 'text-min'} %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_anio').val('2012');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>

        <th style="width:7%" class="text-min">
          <%= label_tag nil, "C&oacute;digo Establecimiento".html_safe%><br />
          <%= text_field_tag :form_buscar_matriculaciones_educacion_media_codigo_establecimiento, nil, :class => 'text-min' %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_codigo_establecimiento').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>

        <th style="width:7%" class="text-min">
          <%= label_tag nil, "C&oacute;digo de Instituci&oacute;n".html_safe %><br />
          <%= text_field_tag :form_buscar_matriculaciones_educacion_media_codigo_institucion, nil, :class => 'text-min' %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_codigo_institucion').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>

        <th style="width:7%" class="text-min">
          <%= label_tag "Institución"%><br />
          <%= text_field_tag :form_buscar_matriculaciones_educacion_media_nombre_institucion, nil, :class => 'text-min' %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_nombre_institucion').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>


        <th style="width:7%" class="text-min">
          <%= label_tag "Departamento"%><br />
          <%= text_field_tag :form_buscar_matriculaciones_educacion_media_nombre_departamento, nil, :class => 'text-min' %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_nombre_departamento').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>

        <th style="width:7%" class="text-min">
          <%= label_tag "Distrito"%><br />
          <%= text_field_tag :form_buscar_matriculaciones_educacion_media_nombre_distrito, nil, :class => 'text-min' %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_nombre_distrito').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>

        <th style="width:7%" class="text-min">
          <%= label_tag "Barrio/Localidad"%><br />
          <%= text_field_tag :form_buscar_matriculaciones_educacion_media_nombre_barrio_localidad, nil, :class => 'text-min' %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_nombre_barrio_localidad').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>

        <th style="width:7%" class="text-min">
          <%= label_tag "Zona"%><br />
          <%= text_field_tag :form_buscar_matriculaciones_educacion_media_nombre_zona, nil, :class => 'text-min' %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_nombre_zona').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>


        <th style="width:7%" class="text-min">
          <%= label_tag "Sector o Tipo de Gestión"%><br />
          <%= text_field_tag :form_buscar_matriculaciones_educacion_media_sector_o_tipo_gestion, nil, :class => 'text-min' %>
          <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_sector_o_tipo_gestion').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%>
        </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "Cantidad de Matriculados\n Científico".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_educacion_media_matricula_cientifico_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_educacion_media_matricula_cientifico, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_matricula_cientifico').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%><br />
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "Cantidad de Matriculados\n Técnico".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_educacion_media_matricula_tecnico_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_educacion_media_matricula_tecnico, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_matricula_tecnico').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%><br />
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "Cantidad de Matriculados\n Media Abierta".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_educacion_media_matricula_media_abierta_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_educacion_media_matricula_media_abierta, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_matricula_media_abierta').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%><br />
      </th>


      <th style="width:10%;" class="text-min">
        <%= label_tag nil, "Cantidad de Matriculados\n Formación Profesional Media".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_educacion_media_matricula_formacion_profesional_media_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_educacion_media_matricula_formacion_profesional_media, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_educacion_media_matricula_formacion_profesional_media').val('');$('#form-buscar-matriculaciones-educacion-media').submit();" ,:style => "color:#fff;"%><br />
      </th>

      <% end %>

    </tr>

    <tbody id="matriculaciones-educacion-media-lista"></tbody>

  </table>
  </div>

   <table class="table table-striped table-bordered table-hover">
    <tbody id="matriculaciones-educacion-media-footer"></tbody>
  </table>
 

<script type="text/javascript">

  $("#form-buscar-matriculaciones-educacion-media").bind("submit", function(){

      $("#matriculaciones-educacion-media-lista").html("<%= escape_javascript("<tr><td style='7'><p class='none-result'>#{image_tag('ajax-loader0.gif')}</p>").html_safe %></td></tr>")    
  
  })

  $("#form_buscar_matriculaciones_educacion_media_anio").bind('change',function(){
      
    $("#form-buscar-matriculaciones-educacion-media").submit();
      
  })

 $(".table-bordered :input").bind('change',function(){     

    $("#form-buscar-matriculaciones-educacion-media").submit();
      
  })

  //LIMPIAR FORMULARIO DE BUSQUEDA
  function limpiar_form_buscar_matriculaciones_educacion_media(){
    
    $("#form_buscar_matriculaciones_educacion_media_anio").val("2012");
    $("#form_buscar_matriculaciones_educacion_media_nombre_departamento").val("");
    $("#form_buscar_matriculaciones_educacion_media_nombre_distrito").val("");
    $("#form_buscar_matriculaciones_educacion_media_nombre_barrio_localidad").val("");
    $("#form_buscar_matriculaciones_educacion_media_nombre_zona").val("");
    $("#form_buscar_matriculaciones_educacion_media_codigo_establecimiento").val("");
    $("#form_buscar_matriculaciones_educacion_media_nombre_institucion").val("");
    $("#form_buscar_matriculaciones_educacion_media_sector_o_tipo_gestion").val("");
    $("#form_buscar_matriculaciones_educacion_media_matriculados_cientifico_mayor").val("");
    $("#form_buscar_matriculaciones_educacion_media_matriculados_cientifico_menor").val("");
    $("#form_buscar_matriculaciones_educacion_media_matriculados_tecnico_mayor").val("");
    $("#form_buscar_matriculaciones_educacion_media_matriculados_tecnico_menor").val("");
    $("#form_buscar_matriculaciones_educacion_media_matriculados_media_abierta_mayor").val("");
    $("#form_buscar_matriculaciones_educacion_media_matriculados_media_abierta_menor").val("");
    $("#form_buscar_matriculaciones_educacion_media_matriculados_formacion_profesional_media_mayor").val("");
    $("#form_buscar_matriculaciones_educacion_media_matriculados_formacion_profesional_media_menor").val("");


    $("#form-buscar-matriculaciones-educacion-media").submit();

  }  

  limpiar_form_buscar_matriculaciones_educacion_media();

</script>

