<% if desembolsos.present? %>

  <% desembolsos.each do |d| %>

    <tr>
        <td><div><%= d.periodo %></div></td>
        <td><div><%= d.pago %></div></td>
        <td><div><%= d.codigo_nautilus %></div></td>
        <td style="font-weight:bold;">
            <div><%= link_to d.codigo_establecimiento, mec_custom_url(id_establecimientos_path(d.codigo_establecimiento)), id:'tab-establecimiento', target: '_blank', style: 'font-weight:bold;', title: 'Visualizar URI' %>
        </td>  
        <td class="tab-institucion"><div><%= link_to d.codigo_institucion.gsub(".","").to_i, mec_custom_url(id_instituciones_path(d.codigo_institucion.gsub(".","").to_i)), id:'tab-institucion', target: '_blank', style: 'font-weight:bold;', title: 'Visualizar URI' %></div></td>
        <td><div><%= d.denominacion_institucion %></div></td>
        <td><div><%= d.nombre_departamento %></div></td>
        <td><div><%= d.nombre_distrito %></div></td>
        <td><div><%= d.codigo_persona %></div></td>
        <td><div><%= d.nombre_persona %></div></td>
        <td><div><%= d.tipo_especialidad %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(d.total, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(d.primer_monto_capital, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(d.primer_monto_corriente, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(d.segundo_monto_capital, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(d.segundo_monto_corriente, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;font-weight: bold;"><div><%= number_to_currency(d.primer_monto_capital + d.primer_monto_corriente + d.segundo_monto_capital + d.segundo_monto_corriente, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;font-weight: bold;"><div><%= number_to_percentage(obtener_porcentaje_rendicion(d.periodo, d.pago, d.codigo_nautilus, d.codigo_persona, d.id_tipo_especialidad), precision: 2) %></div></td>
        <td style="width:20px;"><%= link_to_desembolsos_detalles(d)  %></td>

    </tr>

  <% end %>
<% end %>

