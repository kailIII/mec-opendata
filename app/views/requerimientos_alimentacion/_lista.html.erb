
<% if requerimientos_alimentacion.present? %>
  
  <% requerimientos_alimentacion.each do |i| %>

    <tr>
      <td class="text-min"><div><%= i.periodo %></div></td>
      <td class="text-min"><div><%= i.nombre_departamento %></div></td>
      <td class="text-min"><div><%= i.nombre_distrito %></div></td>
      <td class="text-min"><div><%= i.numero_prioridad %></div></td>
      <td class="text-min" style="font-weight:bold;">
        <div><%= link_to i.codigo_establecimiento, mec_custom_url(id_establecimientos_path(i.codigo_establecimiento)), target: '_blank', style: 'font-weight:bold;', title: 'Visualizar URI' %>
      </td>  
      <td class="text-min"><div><%= link_to i.codigo_institucion.gsub(".","").to_i, mec_custom_url(id_instituciones_path(i.codigo_institucion.gsub(".","").to_i)), target: '_blank', style: 'font-weight:bold;', title: 'Visualizar URI' %></div></td>
      <td class="text-min"><div><%= i.nombre_institucion %></div></td>
      <td class="text-min"><div><%= i.nombre_zona %></div></td>
      <td class="text-min"><div><%= i.espacio_cocina_comedor %></div></td>       
      <td class="text-min"><div><%= i.doble_escolaridad %></div></td>       
      <td class="text-min"><div><%= i.porcentaje_ausentismo %></div></td>       
      <td class="text-min"><%= i.tipo_alimentacion_escolar %></td>         
      <td class="text-min"><%= i.tipo_merienda %></td>
      <td class="text-min"><%= i.proveedor_merienda %></td>
      <td class="text-min"><%= i.cantidad_meses_merienda %></td>
      <td class="text-min"><%= i.tipo_almuerzo %></td>
      <td class="text-min"><%= i.proveedor_almuerzo %></td>
      <td class="text-min"><%= i.cantidad_meses_almuerzo %></td>
      <td class="text-min"><%= i.forma_obtencion_almuerzo %></td>
      <td class="text-min"><%= i.numero_beneficiados %></td>
      <td class="text-min"><%= i.justificacion %></td>
    </tr>
  
  <% end %>
  
  <% else %>

  <tr>
    
    <td colspan="21"><p class='none-result'>No se han encontrado resultados.</p></td>
  
  </tr>

<% end %>
