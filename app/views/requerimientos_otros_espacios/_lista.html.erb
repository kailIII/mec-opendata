
<% if requerimientos_otros_espacios.present? %>
  
  <% requerimientos_otros_espacios.each do |i| %>

    <tr>
      <td><div><%= i.periodo %></div></td>
      <td><div><%= i.nombre_departamento %></div></td>
      <td><div><%= i.nombre_distrito %></div></td>
      <td><div><%= i.numero_prioridad %></div></td>
      <td style="font-weight:bold;">
        <div><%= link_to i.codigo_establecimiento, mec_custom_url(id_establecimientos_path(i.codigo_establecimiento)), id:'tab-establecimiento', target: '_blank', style: 'font-weight:bold;', title: 'Visualizar URI' %>
      </td>  
      <td><div><%= link_to i.codigo_institucion.gsub(".","").to_i, mec_custom_url(id_instituciones_path(i.codigo_institucion.gsub(".","").to_i)), id:'tab-institucion', target: '_blank', style: 'font-weight:bold;', title: 'Visualizar URI' %></div></td>
      <td><div><%= i.nombre_institucion %></div></td>
      <td><div><%= i.nombre_zona %></div></td>
      <td><div><%= i.nivel_educativo_beneficiado %></div></td>          
      <td><div><%= i.cuenta_espacio_para_construccion %></div></td>
      <td><%= i.nombre_espacio %></td>
      <td><%= i.tipo_requerimiento_infraestructura %></td>       
      <td><%= i.cantidad_requerida %></td>              
      <td><%= i.numero_beneficiados %></td>  
      <td><%= i.justificacion %></td>
    </tr>
  
  <% end %>
  
  <% else %>

  <tr>
    
    <td colspan="19"><p class='none-result'>No se han encontrado resultados.</p></td>
  
  </tr>

<% end %>
