<% if rendiciones.present? %>

  <% rendiciones.each do |r| %>

    <tr>
        <td><div><%= r.nivel %></div></td>
        <td><div><%= r.periodo %></div></td>
        <td><div><%= r.pago %></div></td>
        <td><div><%= r.codigo_nautilus %></div></td>
        <td style="font-weight:bold;">
            <div><%= link_to r.codigo_establecimiento, mec_custom_url(id_establecimientos_path(r.codigo_establecimiento)), id:'tab-establecimiento', target: '_blank', style: 'font-weight:bold;', title: 'Visualizar URI' if r.codigo_establecimiento.present? %>
        </td>  
        <td class="tab-institucion"><div><%= link_to r.codigo_institucion.gsub(".","").to_i, mec_custom_url(id_instituciones_path(r.codigo_institucion.gsub(".","").to_i)), id:'tab-institucion', target: '_blank', style: 'font-weight:bold;', title: 'Visualizar URI' if r.codigo_institucion.present? %></div></td>
        <td><div><%= r.denominacion_institucion %></div></td>
        <td><div><%= r.nombre_departamento %></div></td>
        <td><div><%= r.nombre_distrito %></div></td>
        <td><div><%= r.codigo_persona %></div></td>
        <td><div><%= r.nombre_persona %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(r.monto_desembolso_834, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(r.monto_rendicion_834, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(r.saldo_rendicion_834, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= r.estado_transferencia_834 %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(r.monto_desembolso_894, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(r.monto_rendicion_894, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= number_to_currency(r.saldo_rendicion_894, :unit => '', :precision => 0) %></div></td>
        <td style="text-align: right;"><div><%= r.estado_transferencia_894 %></div></td>

    </tr>

  <% end %>
<% end %>

