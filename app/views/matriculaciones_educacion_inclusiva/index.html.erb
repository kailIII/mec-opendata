      <meta itemprop="url" content= <%= mec_custom_url(data_matriculaciones_educacion_inclusiva_url) %> >
      <meta itemprop="temporal" style="display: none;" content="2012-01-01/2013-12-31"/>
      <span itemprop="provider" itemscope itemtype="https://schema.org/Organization" style="display: none;">
