$("#matriculaciones-educacion-inclusiva-lista, #matriculaciones-educacion-inclusiva-lista-clone").html("<%= escape_javascript(render :partial => "lista", :locals => { :matriculaciones_educacion_inclusiva => @matriculaciones_educacion_inclusiva}) %>");

<% if @matriculaciones_educacion_inclusiva.present? %>

  <% parametros = "?form_buscar_matriculaciones_educacion_inclusiva[anio]=#{params[:form_buscar_matriculaciones_educacion_inclusiva][:anio]}"
     parametros += "&form_buscar_matriculaciones_educacion_inclusiva_codigo_establecimiento=#{params[:form_buscar_matriculaciones_educacion_inclusiva_codigo_establecimiento]}"
