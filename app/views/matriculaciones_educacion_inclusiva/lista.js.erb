	parametros += "&form_buscar_matriculaciones_educacion_inclusiva_nombre_institucion=#{params[:form_buscar_matriculaciones_educacion_inclusiva_nombre_institucion]}"
     parametros += "&form_buscar_matriculaciones_educacion_inclusiva_sector_o_tipo_gestion=#{params[:form_buscar_matriculaciones_educacion_inclusiva_sector_o_tipo_gestion]}"
