     parametros += "&page=#{(params[:page].present? ? params[:page] : 1)}" %>

<% end %>


$("#matriculaciones-educacion-inclusiva-footer").html("<%= escape_javascript(render :partial => "footer", :locals => { :matriculaciones_educacion_inclusiva => @matriculaciones_educacion_inclusiva}) %>");
