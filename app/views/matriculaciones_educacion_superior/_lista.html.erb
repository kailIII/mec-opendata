
<% if matriculaciones_educacion_superior.present? %>

  <% matriculaciones_educacion_superior.each do |e| %>

    <tr>

      <td><%= e.anio %></td>
      <td><b><%= link_to e.codigo_establecimiento, mec_custom_url(id_establecimientos_path(e.codigo_establecimiento)), target: '_blank',style: 'font-weight:bold;', title: 'Visualizar URI' %></b></td>
      <td><b><%= link_to e.codigo_institucion, mec_custom_url(id_instituciones_path(e.codigo_institucion)), target: '_blank',style: 'font-weight:bold;', title: 'Visualizar URI' %></b></td>
      <td><%= e.nombre_institucion %></td>
      <td><%= e.nombre_departamento %></td>
      <td><%= e.nombre_distrito %></td>
      <td><%= e.nombre_barrio_localidad %></td>
      <td><%= e.nombre_zona %></td>
      <td><%= e.sector_o_tipo_gestion %></td>
      <td><%= e.matricula_ets %></td>       
      <td><%= e.matricula_fed %></td>       
      <td><%= e.matricula_fdes %></td>       
      <td><%= e.matricula_pd %></td>       
    </tr>

  <% end %>

<% else %>

  <tr>

    <td colspan="13"><p class='none-result'>No se han encontrado resultados.</p></td>

  </tr>

<% end %>
