<h2>
	<span itemprop="name">Matriculaciones Educación Inicial</span>
  <meta itemprop="description" content="Se refiere a la cantidad de matriculados en las etapas maternal, prejardín, jardín y preescolar con sus correspondientes códigos identificatorios."/>
  <meta itemprop="url" content="http://www.mec.gov.py/data/matriculaciones_inicial">
  <meta itemprop="temporal" style="display: none;" content="2012-01-01/2012-12-31"/>
  <span itemprop="catalog" itemscope itemtype="https://schema.org/DataCatalog" style="display: none;">
    <meta itemprop="name" content="Datos Abiertos del Ministerio de Educación y Cultura">
    <meta itemprop="url" content="http://datos.mec.gov.py/">
    <meta itemprop="description" content="Datos Abiertos del Ministerio de Educación y Cultura">
  </span>
  <span itemprop="provider" itemscope itemtype="https://schema.org/Organization" style="display: none;">
    <meta itemprop="name" content="Ministerio de Educación y Cultura">
    <meta itemprop="url" content="http://www.mec.gov.py/">
    <meta itemprop="email" content="datosabiertos@mec.gov.py">
  </span>
  <span itemprop="creator" itemscope itemtype="https://schema.org/Person" style="display: none;">
    <meta itemprop="name" content="Juan Barrios">
    <meta itemprop="email" content="datosabiertos@mec.gov.py">
  </span>
  <meta itemprop="license" style="display: none;" content="https://creativecommons.org/licenses/by/4.0/legalcode"/>
  <meta itemprop="keywords" style="display: none;" content="educación,matriculaciones,educación inicial"/>
  <meta itemprop="version" style="display: none;" content="1.0"/>
  
  <span style="float:right;">
    <%= link_to "Ver diccionario", mec_custom_url(def_matriculaciones_inicial_url), :title => "Ir al diccionario", :style => "font-size:12px;" %>
    <%= link_to image_tag("def_icon.png", :style => "width:24px;" ), url_def_matriculaciones_inicial, :title => "Ir al diccionario" %>
    <%= link_to "Inicio", mec_custom_url(index_url), :title => "Ir al inicio", :style => "font-size:12px;" %>
    <%= link_to image_tag("home.png", :style => "width:24px;"), mec_custom_url(index_url), :title => "Ir al inicio" %>
  </span>

</h2>

<div class="table-outter">
  <table id="matriculaciones-inicial-table" class="table table-striped table-bordered table-hover" style="margin-bottom: 0px; width: 2500px; max-width: none">

  <tr>

    <%= form_tag mec_custom_url(data_matriculaciones_inicial_lista_url(nil)), :remote => true, :id => "form-buscar-matriculaciones-inicial" do  %>

      <th style="width:5%;" class="text-min celda-derecha">
        <%= label_tag "Periodo" %><br />
        <%= select :form_buscar_matriculaciones_inicial, :anio, [2012], {:class => 'text-min'} %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_anio').val('2012');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "C&oacute;digo Establecimiento".html_safe%><br />
        <%= text_field_tag :form_buscar_matriculaciones_inicial_codigo_establecimiento, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_codigo_establecimiento').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "C&oacute;digo Instituci&oacute;n".html_safe%><br />
        <%= text_field_tag :form_buscar_matriculaciones_inicial_codigo_institucion, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_codigo_institucion').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag "Nombre Institución"%><br />
        <%= text_field_tag :form_buscar_matriculaciones_inicial_nombre_institucion, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_nombre_institucion').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag "Departamento"%><br />
        <%= text_field_tag :form_buscar_matriculaciones_inicial_nombre_departamento, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_nombre_departamento').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag "Distrito"%><br />
        <%= text_field_tag :form_buscar_matriculaciones_inicial_nombre_distrito, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_nombre_distrito').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag "Barrio/Localidad"%><br />
        <%= text_field_tag :form_buscar_matriculaciones_inicial_nombre_barrio_localidad, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_nombre_barrio_localidad').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>
      
      <th style="width:6%;" class="text-min">
        <%= label_tag "Zona"%><br />
        <%= text_field_tag :form_buscar_matriculaciones_inicial_nombre_zona, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_nombre_zona').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag "Sector o Tipo Gestión"%><br />
        <%= text_field_tag :form_buscar_matriculaciones_inicial_sector_o_tipo_gestion, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_sector_o_tipo_gestion').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%>
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "Maternal".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_inicial_maternal_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_inicial_maternal, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_maternal').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%><br />
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "Pre-Jardin".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_inicial_prejardin_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_inicial_prejardin, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_prejardin').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%><br />
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "Jardin".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_inicial_jardin_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_inicial_jardin, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_jardin').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%><br />
      </th>

      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "Pre-Escolar".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_inicial_preescolar_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_inicial_preescolar, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_preescolar').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%><br />
      </th>


      <th style="width:7%;" class="text-min">
        <%= label_tag nil, "Total Matriculados".html_safe%><br />
        <%= select_tag(:form_buscar_matriculaciones_inicial_total_matriculados_operador, options_for_select(['>', '<', '='])) %>
        <%= text_field_tag :form_buscar_matriculaciones_inicial_total_matriculados, nil, :class => 'text-min' %>
        <%= link_to_function image_tag("delete-icon.png", :style => "width:16px;"), "$('#form_buscar_matriculaciones_inicial_total_matriculados').val('');$('#form-buscar-matriculaciones-inicial').submit();" ,:style => "color:#fff;"%><br />
      </th>

 <% end %>

  </tr>

  <tbody id="lista"></tbody>

</table>

</div>

   <table class="table table-striped table-bordered table-hover">
    <tbody id="matriculaciones-inicial-footer"></tbody>
  </table>

<script type="text/javascript">

  $("#form-buscar-matriculaciones-inicial").bind("submit", function(){

      $("#lista").html("<%= escape_javascript("<tr><td style='7'><p class='none-result'>#{image_tag('ajax-loader0.gif')}</p>").html_safe %></td></tr>")    
  
  })

  $("#form_buscar_matriculaciones_inicial_anio").bind('change',function(){
      
    $("#form-buscar-matriculaciones-inicial").submit();
      
  })

 $(".table-bordered :input").bind('change',function(){
      
    $("#form-buscar-matriculaciones-inicial").submit();
      
  })

  //LIMPIAR FORMULARIO DE BUSQUEDA
  function limpiar_form_buscar_matriculaciones_inicial(){
    
    $("#form_buscar_matriculaciones_inicial_anio").val("2012");
    $("#form_buscar_matriculaciones_inicial_codigo_establecimiento").val("");
    $("#form_buscar_matriculaciones_inicial_nombre_departamento").val("");
    $("#form_buscar_matriculaciones_inicial_nombre_distrito").val("");
    $("#form_buscar_matriculaciones_inicial_nombre_zona").val("");
    $("#form_buscar_matriculaciones_inicial_nombre_barrio_localidad").val("");
    $("#form_buscar_matriculaciones_inicial_nombre_institucion").val("");
    $("#form_buscar_matriculaciones_inicial_sector_o_tipo_gestion").val("");
    $("#form_buscar_matriculaciones_inicial_maternal").val("");
    $("#form_buscar_matriculaciones_inicial_prejardin").val("");
    $("#form_buscar_matriculaciones_inicial_jardin").val("");
    $("#form_buscar_matriculaciones_inicial_preescolar").val("");
    $("#form_buscar_matriculaciones_inicial_total_matriculados").val("");
    $("#form-buscar-matriculaciones-inicial").submit();

  }  

 limpiar_form_buscar_matriculaciones_inicial();

</script>

